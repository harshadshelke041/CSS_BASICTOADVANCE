.content5{
    width: 100%;
    padding: 80px 6%;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    gap: 40px;
    flex-wrap: wrap;
}


.content5 > div{
    flex: 1 1 200px; 
    min-width: 200px;
}


.content5 .col1{
    display: flex;
    flex-direction: column;
    gap: 10px;
    
}

.content5 .col1 img{
    height: 40px;
    width: 40px;
}

.col1 p{
    font-size: 16px;
    color: white;
    font-weight: 300;
    text-align: center;
}

.col1 .logo a{
    text-decoration: none;
    color: white;
    font-size: 18px;
    margin-left: 10px;
}

.content5 .col2{
    display: flex;
    flex-direction: column;
}

.content5 .col2 h3{
    font-size: 18px;
    font-weight: 600;
    color: white;
    text-transform: uppercase;
}

.content5 .tags{
    margin-top: 15px;
    display: flex;
    flex-direction: column;
    gap: 10px;
    
}

.content5 .tags a{
    text-decoration: none;
    color: rgb(241, 233, 233);
    font-size: 16px;
    font-weight: 300;
}

.content5 .col3 h3{
    color: white;
    text-transform: uppercase;
    font-size: 16px;
    font-weight: 600;
}

.content5 .col4 h3{
    color: white;
    text-transform: uppercase;
    font-size: 16px;
    font-weight: 600;
}


.content5 .info{
   margin:20px 0;
}

.content5 .info p{
    color: white;
}

.content5 .info a{
    text-decoration: none;
    color: white;
    font-size: 15px;
}

.content5  .col4 a{
    text-decoration: none;
    color: white
}

.footer{
    margin: 50px;
    text-align: center;
}

.footer h4{
  font-weight: 700;
  color: white;
}

.footer p{
    font-weight: 800;
    font-size: 14px;
    color: white;
}

@media (max-width: 900px) {

    .content5{
        padding: 60px 6%;
        justify-content: center;
    }

    .content5 > div{
        width: 45%;
    }

    .content5 .col1{
        align-items: center;
        text-align: center;
    }
}


@media (max-width: 600px) {

    .content5{
        flex-direction: column;
        align-items: center;
        padding: 40px 5%;
        gap: 30px;
    }

    .content5 > div{
        width: 100%;
        text-align: center;
    }

    .content5  .col2, .col3, .col4 {
        margin-top: 30px;
    }

    .content5 .tags{
        align-items: center;
    }

    .content5 .logo{
        justify-content: center;
    }
}

@media (max-width: 400px) {

    .content5 .col2 h3,
    .content5 .col3 h3,
    .content5 .col4 h3{
        font-size: 14px;
    }

    .content5 .tags a,
    .content5 .info a,
    .content5 .info p{
        font-size: 14px;
    }
}
